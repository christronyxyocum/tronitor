#!/usr/bin/expect -f
#!/usr/bin/env bash

set timeout 10

set apiKey $::env(travisSCApiKey)
set username $::env(travisSCUsername)

spawn sudo bash -x Travis/travis_usmu.sh -l

expect "Enter your provider: "
send -- "statuscake\r"

expect "Enter your API key: "
send -- "$apiKey\r"

expect "Enter your username: "
send -- "$username\r"

expect eof
