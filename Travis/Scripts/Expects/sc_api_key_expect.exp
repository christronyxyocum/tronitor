#!/usr/bin/expect -df
#!/usr/bin/env bash

set timeout 10

set apiKey $::env(travisSCApiKey)
set username $::env(travisSCUsername)

spawn bash Travis/Scripts/travis_tronitor.sh -l

expect "Enter your provider: "
send -- "StatusCake\r"

expect "Enter your API key: "
send -- "$apiKey\r"

expect "Enter your username: "
send -- "$username\r"

expect eof
