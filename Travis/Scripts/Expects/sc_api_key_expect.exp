#!/usr/bin/expect -df
#!/usr/bin/env bash

set timeout 10

set apiKey $::env(travisSCApiKey)
set username $::env(travisSCUsername)

spawn sudo bash Travis/Scripts/travis_tronitor.sh -l

expect "Enter your provider: \r"
send -- "StatusCake\r"

expect "Enter your API key: \r"
send -- "$apiKey\r"

expect "Enter your username: \r"
send -- "$username\r"

expect eof
